#include <iostream>
using namespace std;

int main()
{
	//rosnaco
	bool IsSorted;
	int a,b=0;
	int tab[9]={3,0,2,10,15,4,1,20,19};
	while(b<9){
		for(int i=0;i<10;i++){
		if(tab[i]>tab[i+1]){
			a=tab[i];
			tab[i]=tab[i+1];
			tab[i+1]=a;
		}
		IsSorted=false;
		cout<<i;
	}
	b++;
	if(true==IsSorted)break;
	cout<<i;
	
}
for(int i=0;i<9;i++)cout<<tab[i]<<endl;
	
	
	return 0;
}
